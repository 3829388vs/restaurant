.title {
  font-family: PingFang SC;
  font-weight: 600;
  color: #e017d0;
  font-size: 42rpx;
  margin-bottom: 10rpx;
}
.swiper{
  height: 330rpx;
  width: 550rpx;
  margin-top: 20rpx;
}
.food-text{
  padding-top: 30rpx;
  font-size: 40rpx;
  color: #ffc9fa;
}

.login {
  width: 700rpx;
  background-color: rgba(230, 230, 230, 0.8);
  border-radius: 32rpx;
  padding: 10rpx 50rpx 60rpx 10rpx;
  display:flex;
  flex-wrap: wrap;
  justify-content: left;
  margin-top: 60rpx;
}

.username{
  position: absolute;
  top: 10rpx;
  left: 14rpx;
  color: #ffffff;
  font-size: 36rpx;
}
.username label{
  color: #ffffff;
  padding: 0rpx 8rpx;
}

.login_box {
  width: 150rpx;
  height: 130rpx;
  margin-top: 40rpx;
  margin-bottom: 26rpx;
  padding-left: 24rpx;
}
.login_box2 {
  width: 150rpx;
  height: 130rpx;
  margin-top: 40rpx;
  margin-bottom: 26rpx;
  padding-left: 24rpx;
}
.notice{
  position: absolute;
  width: 100%;
  padding: 12rpx 0rpx;
  top: 80rpx;
  color: #ff0080;
  font-weight: bold;
  background-color: rgba(255, 255, 255, 0.2);
  overflow:hidden;
  height: 36rpx;
}
